<Window x:Class="TestAgent.Manager.WarningView"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="WarningView" Height="134" Width="312" ResizeMode="NoResize" ShowInTaskbar="False">
    <Grid>
        <Button x:Name="Ok" Content="OK" IsDefault="True" ToolTip="Add" HorizontalAlignment="Center" Height="25" Margin="0,0,0,0" VerticalAlignment="Bottom" Width="25" Style="{StaticResource {x:Static ToolBar.ButtonStyleKey}}"/>
        <TextBlock x:Name="Message" Height="53" Margin="0,10,0,0" TextWrapping="Wrap" VerticalAlignment="Top" TextAlignment="Center"/>
    </Grid>
</Window>
